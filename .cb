explorer:
  rows:
    - style:
        align-items: center
        column-gap: 6px
      items:
      - type: container
        style:
          background: var(--vscode-textBlockQuote-background)
          height: 34px
          width: 34px
          margin: 2px
          border-radius: 4px
          # transform: scale(0.8)
          border: 1px solid var(--vscode-interactive-inactiveCodeBorder)
        items:
        - type: text
          text: âŸ‘
          style:
            font-size: 42px
            line-height: 32px
            transform: translateY(-4px) translateX(0.7px)
            width: 30px
            text-align: center
            color: var(--vscode-charts-blue)
      - type: container
        items:
        - type: text
          text: DJITSU
          style:
            font-weight: bold
        - type: text
          text: M2
          style:
            font-size: 0.8em

    # tools/ui-dev
    - style:
        padding: 0 0 0 17px
      glob:
        - "tools/ui-dev/**"
        - "tools/*"
        - ".*"
        - "*.*"
      items:
      - type: svg
        svg: node_modules/@ladle/react/lib/app/favicon.svg
        style: 
          flex: 0 0 auto
          background: var(--vscode-textBlockQuote-background)
          padding: 2px
          margin-right: 4px
          border-radius: 4px
          width: 18px
          height: 18px
          display: flex
          align-items: center
          justify-content: center

      - type: container
        style:
          display: flex
          column-gap: 12px
          width: 100%
          padding: 0 12px 0 0
        items:
          - text: "tools/ui-dev"
            style:
              margin-right: auto
              background: var(--vscode-inputValidation-infoBackground)
              padding: 1px 5px
              border-radius: 4px
            type: text
          - markdown: "[localhost:61000](http://localhost:61000)"
            type: markdown


